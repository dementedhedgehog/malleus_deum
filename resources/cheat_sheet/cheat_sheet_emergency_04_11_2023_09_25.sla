        <PAGEOBJECT XPOS="997.165984251969" YPOS="368" OwnPage="0" ItemID="1853609082" PTYPE="4" WIDTH="253.385826771654" HEIGHT="453.891023622047" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L253.386 0 L253.386 453.891 L0 453.891 L0 0 Z" copath="M0 0 L253.386 0 L253.386 453.891 L0 453.891 L0 0 Z" gXpos="997.165984251969" gYpos="368" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle LINESP="9" FONTSIZE="9"/>
                <breakline/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="15" CH="Death Saves"/>
                <breakline/>
                <breakline/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="≤1 Dying"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="They will be dead at the end of the next"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="round unless someone intervenes.  If all else fails they can make a final"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="Std 2d12 roll for divine intervention vs DC 21, if successful or a boon occurs, then"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="some god spares them for reasons that the god will make known to the character at"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="some time in the near future.  If successful the character gains an aspect reflecting"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="their obligation to this deity.  In the case of a Bane the divine intervention"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="is from a malicious source seeking to use the character as a pawn in their nefarious"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="plans.  The character may reject the intervention in either case."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <breakline/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="2 Maimed"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Suffers some permanent disability, eg. They have lost an eye or a hand, their face has been badly burned etc.  Gain the Checked condition"/>
                <ITEXT CH=" "/>
                <ITEXT FONTSIZE="9" CH="and 1 "/>
                <ITEXT CH="Health"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <breakline/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="3-5 Bleeding Out Fast"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Roll again next round with a -5 cummulative modifier."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="6-8 Bleeding Out"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Roll again next round with a -3 cummulative modifier."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="9-13 It Doesn't Look Good"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Roll again next round."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="14-15 Out Cold"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="The character gets the Unconscious condition and 1 health."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="16-17 Coming To"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Roll again next round with a +3 cummulative modifier."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <breakline/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="18 Waking Up"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Roll again next round with a +5 cummulative modifier."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="19-20 Where Am I?"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Character gains the Dazed condition, the Checked condition and 1 health."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="21-22 A Flesh Wound"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Recovers 1 health and gains the Dazed condition."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="23 I'm Fine"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Recovers 2 health."/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONT="Cloister Black Light" FONTSIZE="12" CH="≥24 Never Been Better"/>
                <para LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
                <ITEXT FONTSIZE="9" CH="Recovers 3 health and gets an additional 5 Stamina."/>
                <trail LINESP="9" ParagraphEffectOffset="8.50393700787402" ParagraphEffectIndent="1" DROP="0" Bullet="0" BulletStr="•" Numeration="0"/>
            </StoryText>
        </PAGEOBJECT>
